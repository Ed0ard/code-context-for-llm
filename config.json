{
    "max_file_size_kb": 1024,
    "exclude": {
        "extensions": [
            ".env",
            ".pyc",
            ".log",
            ".git",
            ".gitignore"
        ],
        "files": [
            "LICENSE",
            "file_inutile.txt"
        ]
    },
    "exclude_structure": {
        "extensions": [
            ".pdf",
            ".png",
            ".jpg",
            ".jpeg"
        ],
        "directories": [
            "__pycache__",
            ".git",
            "venv"
        ],
        "files": [
            "LICENSE"
        ]
    }
}